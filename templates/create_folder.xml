{% extends "base_request.xml" %}

{% block body %}
    <CreateFolder xmlns="http://schemas.microsoft.com/exchange/services/2006/messages">
      <ParentFolderId>
        <t:DistinguishedFolderId Id="{{ parent_folder_id }}"/>
      </ParentFolderId>

      <Folders>
        {% for name in names %}
        <t:Folder>
           <t:DisplayName>
               {{ name }}
           </t:DisplayName>
        </t:Folder>
	{% end %}
      </Folders>
    </CreateFolder>
{% end %}
